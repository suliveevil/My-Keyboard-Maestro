						<string>#!/usr/bin/perl

use Lingua::EN::Fathom;

$sample1 = $ENV{KMVAR_LocalText};

my $text = Lingua::EN::Fathom-&gt;new();
$text-&gt;analyse_block($sample1,1);
 
%u_words = $text-&gt;unique_words;

print "&lt;table border=0 cellpadding=3 cellspacing=0 width=100%&gt;\n";
foreach $word ( sort { $u_words{$b} &lt;=&gt; $u_words{$a} } keys %u_words ) {
	if ($u_words{$word} &gt; 1 &amp;&amp; $word !~ /\b(the|and|or|an?|in|to|of|on|for|by|in|but)\b/) {
        	print "&lt;tr&gt;\n&lt;td align=\"right\"&gt;$u_words{$word}&lt;/td&gt;&lt;td&gt;$word&lt;/td&gt;\n&lt;/tr&gt;\n";
	}
}
print "&lt;/table&gt;\n";</string>
