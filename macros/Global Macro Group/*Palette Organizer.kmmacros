		&lt;!-- ==================== Prorotypes, Helper Functions =============================== --&gt;
		&lt;script type="text/javascript"&gt;
			if (!String.prototype.startsWith) {
				String.prototype.startsWith = function(searchString, position) {
					position = position || 0;
					return this.substr(position, searchString.length) === searchString;
				};
			}

			Node.prototype.toggleClass = function toggleClass(className, include) {
				var classNames = this.className.split(" ");
				var index = classNames.indexOf(className);
				if (include) {
					if (index &lt; 0) {
						classNames.push(className);
					}
				} else {
					if (index &gt;= 0) {
						classNames.splice(index, 1);
					}
				}
				this.className = classNames.join(" ");
			};

			Node.prototype.swapClasses = function swapClasses(className1, className2, useClassName1) {
				function swap(node, name1, name2) {
					var classNames = node.className.split(" ");
					var i = classNames.indexOf(name2);
					if (i &gt;= 0)
						classNames.splice(i, 1);
					if (classNames.indexOf(name1) &lt; 0)
						classNames.push(name1);
					node.className = classNames.join(" ");
				}

				if (useClassName1 === undefined || useClassName1) {
					swap(this, className1, className2);
				} else {
					swap(tis, className2, className1);
				}
			};

			function getKMVariable(name, required) {
				var value = window.KeyboardMaestro.GetVariable(name);
				if (required &amp;&amp; !value)
					throw Error("Variable '" + name + "' not set");
				return value;
			}
		&lt;/script&gt;

		&lt;!-- ============= logDebugMessage and ErrorHandler ========================================= --&gt;
		&lt;script type="text/javascript"&gt;
			var _logDebugMessageEnabled = false;
			var _firstDebugMessage = true;
			function logDebugMessage(message) {
				if (!_logDebugMessageEnabled)
					return;
				try {
					if (_firstDebugMessage) {
						_firstDebugMessage = false;
						window.KeyboardMaestro.Trigger("LogDebugMessage", "=====================================================");
					}
					window.KeyboardMaestro.Trigger("LogDebugMessage", message);
				} catch (e) {
				}
			}

			function dump(obj, desc) {
				logDebugMessage((desc ? desc + ": " : "") + JSON.stringify(obj, null, "\t"));
			}

			var ErrorHandler = (function() {
				return {
					handle: function(error, context) {
						this.log(ErrorHandler.format(error, context));
					},

					format: function(error, context) {
						if (context)
							return "Error in '" + context + "':\n" + error.message;
						return error.message;
					},

					log: function(str) {
						logDebugMessage(str);
						document.write(str.split("\n").join("&lt;br /&gt;") + "&lt;br / /&gt;");
					}
				};
			})();
		&lt;/script&gt;

		&lt;!-- ============= Modal Dialogs ================================================== --&gt;
		&lt;script type="text/javascript"&gt;
			function showInfoDialog(message) {
				try {
					// document.getElementById("modalDialogTitle").innerText = title;
					document.getElementById("infoDialogMessage").innerText = message;
					document.getElementById("infoDialog").swapClasses("modalDialogVisible", "modalDialogHidden");
				} catch (e) {
					ErrorHandler.handle(e, "showInfoDialog");
				}
			}

			function closeInfoDialog() {
				try {
					document.getElementById("infoDialog").swapClasses("modalDialogHidden", "modalDialogVisible");
				} catch (e) {
					ErrorHandler.handle(e, "closeInfoDialog");
				}
			}

			function editMacroName() {
				try {
					var selection = getSelectedOptionAndMacro();
					if (!selection)
						return;
					showEditBoxDialog("Edit Macro Name", selection.macro.macroNameNoPrefix, "editMacroName");
				} catch (e) {
					ErrorHandler.handle(e, "editMacroName");
				}
			}

			var _showEditBoxDialogWhenAddingDivider = true;
			function addDivider() {
				try {
					if (_showEditBoxDialogWhenAddingDivider) {
						showEditBoxDialog("Enter Divider Text", _lastDividerString, "addDivider");
					} else {
						addDividerMacro(_lastDividerString);
					}
				} catch (e) {
					ErrorHandler.handle(e, "addDivider");
				}
			}

			function addDividerMacro(text) {
				var macro = {
					macroUUID: "",
					macroName: "",
					prefixNumber: "",
					macroNameNoPrefix: text
				};
				_macrosInfo.macros.push(macro);
				var macroIndex = _macrosInfo.macros.length - 1;

				var sel = document.getElementById("selMacros");
				var selIndex = sel.selectedIndex;
				sel.selectedIndex = addMacroToSelMacros(macroIndex, selIndex &gt;= 0 ? selIndex + 1 : undefined);
				storeLastDivider(text);
				enableControls();
			}

			function deleteDivider() {
				try {
					var selection = getSelectedOptionAndMacro();
					if (selection.macro.macroUUID)
						return;
					var macroIndex = selection.option.value;
					selection.sel.options.remove(selection.optionIndex);
					_macrosInfo.macros.splice(macroIndex, 1);
					enableControls();
				} catch (e) {
					ErrorHandler.handle(e, "deleteDivider");
				}
			}

			var _editBoxDialogShowing = false;
			var _editBoxDialogListenerAssigned = false;
			function showEditBoxDialog(title, text, action) {
				try {
					var input = document.getElementById("editBoxDialogText");

					if (!_editBoxDialogListenerAssigned) {
						_editBoxDialogListenerAssigned = true;
						input.addEventListener("keydown", function(event) {
							switch (event.keyCode) {
								case 13:
									closeEditBoxDialog("OK");
									break;
								case 27:
									closeEditBoxDialog("Cancel");
									break;
							}
						});
					}

					input.value = text;
					document.getElementById("editBoxDialogTitle").innerHTML = title;
					var dialog = document.getElementById("editBoxDialog");
					dialog.dataset.action = action;
					dialog.swapClasses("modalDialogVisible", "modalDialogHidden");
					_editBoxDialogShowing = true;

					window.setTimeout(function() {
						var input = document.getElementById("editBoxDialogText");
						input.focus();
						input.select();
					}, 101);
				} catch (e) {
					ErrorHandler.handle(e, "showEditBoxDialog");
				}
			}

			function closeEditBoxDialog(result) {
				function setErrorMessage(str) {
					document.getElementById("editBoxDialogErrorMessage").innerHTML = str;
				}

				function validateAndStoreMacroName(macroName) {
					var selection = getSelectedOptionAndMacro();
					if (!selection)
						throw Error("Logic Error: No selection found");
					if (!macroName) {
						setErrorMessage("Macro Name is required");
						return false;
					}
					selection.macro.macroNameNoPrefix = macroName;
					selection.option.text = getMacroName(selection.macro);

					if (!selection.macro.macroUUID) {
						storeLastDivider(macroName);
						_showEditBoxDialogWhenAddingDivider = false;
					}

					return true;
				}

				function validateAndAddDivider(text) {
					if (!text) {
						setErrorMessage("Divider Text is required");
						return false;
					}
					addDividerMacro(text);
					_showEditBoxDialogWhenAddingDivider = false;
					return true;
				}

				function validateAndStoreResult(action) {
					var macroName = document.getElementById("editBoxDialogText").value.trim();
					switch (action) {
						case "editMacroName":
							return validateAndStoreMacroName(macroName);
						case "addDivider":
							return validateAndAddDivider(macroName);
						default:
							throw Error("Unexpected action: '" + action + "'");
					}
				}

				// closeEditBoxDialog()
				try {
					var dialog = document.getElementById("editBoxDialog");
					if (result === "OK" &amp;&amp; !validateAndStoreResult(dialog.dataset.action))
						return;
					setErrorMessage("");
					dialog.swapClasses("modalDialogHidden", "modalDialogVisible");

					setTimeout(function() {
						_editBoxDialogShowing = false;
					}, 1);
				} catch (e) {
					ErrorHandler.handle(e, "closeEditBoxDialog");
				}
			} // closeEditBoxDialog()
		&lt;/script&gt;

		&lt;!-- ============= UI Manipulation and Access ===================================== --&gt;
		&lt;script type="text/javascript"&gt;
			function enableControls() {

				function enableControl(id, enabled) {
					document.getElementById(id).toggleClass("disabled", !enabled);
				}

				function showControl(id, show) {
					document.getElementById(id).style.visibility = show ? "visible" : "collapse";
				}

				try {
					var selectedOption = getSelectedOptionAndMacro();
					enableControl("btnUp", selectedOption &amp;&amp; selectedOption.lowestSelectedIndex &gt; 0);
					enableControl("btnDown", selectedOption &amp;&amp; selectedOption.highestSelectedIndex &lt; selectedOption.optionsCount - 1);
					enableControl("btnEditMacroName", selectedOption);
					showControl("btnDeleteDivider", selectedOption &amp;&amp; !selectedOption.macro.macroUUID);
				} catch (e) {
					ErrorHandler.handle(e, "enableControls");
				}
			}

			function getMacroName(macro) {
				try {
					if (macro.prefixNumber)
						return macro.prefixNumber + ")" + macro.macroNameNoPrefix;
					return macro.macroNameNoPrefix;
				} catch (e) {
					ErrorHandler.handle(e, "getMacroName");
				}
			}

			function addMacroToSelMacros(macroIndex, selIndex) {
				try {
					var macro = _macrosInfo.macros[macroIndex];
					var opt = document.createElement("option");
					opt.text = getMacroName(macro);
					opt.value = macroIndex;
					var sel = document.getElementById("selMacros");
					if (selIndex !== undefined) {
						sel.add(opt, selIndex);
					} else {
						sel.add(opt);
						selIndex = sel.options.length - 1;
					}
					return selIndex;
				} catch (e) {
					ErrorHandler.handle(e, "addMacroToSelMacros");
				}
			}

			function getSelectedOptionAndMacro() {
				try {
					var sel = document.getElementById("selMacros");
					var optionIndex = sel.selectedIndex;
					if (optionIndex &lt; 0)
						return null;

					var option;
					var lowestSelectedIndex = 9999;
					var highestSelectedIndex = -1;
					var options = sel.options;
					for (var i = 0; i &lt; options.length; i++) {
						option = options[i];
						if (option.selected) {
							lowestSelectedIndex = Math.min(lowestSelectedIndex, i);
							highestSelectedIndex = Math.max(highestSelectedIndex, i);
						}
					}

					option = sel.options[optionIndex];
					return {
						sel: sel,
						option: option,
						optionIndex: optionIndex,
						macro: _macrosInfo.macros[option.value],
						lowestSelectedIndex: lowestSelectedIndex,
						highestSelectedIndex: highestSelectedIndex,
						optionsCount: sel.options.length
					};
				} catch (e) {
					ErrorHandler.handle(e, "getSelectedOptionAndMacro");
				}
			}

			function applyRenumbering() {

				function setRenumberErrorMessage(str) {
					document.getElementById("spanRenumberErrorMessage").innerHTML = str;
				}

				function isValidNumber(str, min, max) {
					if (str.search(/^\d\d*$/) !== 0)
						return false;
					var num = parseInt(str);
					return num &gt;= min &amp;&amp; num &lt;= max;
				}

				try {
					setRenumberErrorMessage("");
					var maxStartingNumber = 100 - _macrosInfo.macros.length;
					var startingNumber = document.getElementById("inpStartingNumber").value;
					if (!isValidNumber(startingNumber, 1, maxStartingNumber)) {
						setRenumberErrorMessage("Starting Number must be a number between 1 and " + maxStartingNumber + ".");
						return;
					}
					startingNumber = parseInt(startingNumber);

					var increment = document.getElementById("inpIncrement").value;
					if (!isValidNumber(increment, 1, 98)) {
						setRenumberErrorMessage("Increment must be a number greater than 0.");
						return;
					}
					increment = parseInt(increment);

					if (startingNumber + ((_macrosInfo.macros.length - 1) * increment) &gt; 99) {
						setRenumberErrorMessage("These values would create a prefix &gt; 2 digits.");
						return;
					}

					var options = document.getElementById("selMacros").options;
					var prefixNumber = startingNumber;
					for (var i = 0; i &lt; options.length; i++) {
						var option = options[i];
						var macro = _macrosInfo.macros[option.value];
						macro.prefixNumber = (prefixNumber &lt; 10 ? "0" : "") + prefixNumber;
						option.text = getMacroName(macro);
						prefixNumber += increment;
					}
				} catch (e) {
					ErrorHandler.handle(e, "applyRenumbering");
				}
			}

			function moveSelectedItems(up) {
				function swapIf(sel,i1,i2) {
					var swapVar = '';
					if ( ! sel[i1].selected &amp;&amp; sel[i2].selected) {
						swapVar = sel[i2].text;
						sel[i2].text = sel[i1].text;
						sel[i1].text = swapVar;
						swapVar = sel[i2].value;
						sel[i2].value = sel[i1].value;
						sel[i1].value = swapVar;
						sel[i1].selected = true;
						sel[i2].selected = false;
					}
				}

				function moveUp() {
					try {
						var sel = document.getElementById("selMacros");
						var i1=0, i2=1;
						while (i2 &lt; sel.options.length) {
							swapIf(sel,i1++,i2++);
						}
						enableControls();
					} catch (e) {
						ErrorHandler.handle(e, "moveUp");
					}
				}

				function moveDown() {
					try {
						var sel = document.getElementById("selMacros");
						var i1=sel.options.length-1, i2=i1-1;
						while (i1 &gt; 0) {
							swapIf(sel,i1--,i2--);
						}
						enableControls();
					} catch (e) {
						ErrorHandler.handle(e, "moveDown");
					}
				}

				// moveSelectedItems()
				if (up) {
					moveUp();
				} else {
					moveDown();
				}
			} // moveSelectedItems()

			function fillSelect() {
				var sel = document.getElementById("selMacros");
				for (var i = 0; i &lt; _macrosInfo.macros.length; i++) {
					addMacroToSelMacros(i);
				}
	        }
		&lt;/script&gt;

		&lt;!-- ============= Main Body Logic ========================================= --&gt;
		&lt;script type="text/javascript"&gt;
			var _desiredWindowPosition;
			var _macrosInfo;
			var _lastDividerString;
			var _lastDividerCharVarName = "DND__palorg_lastDividerChar";
			var _availWindowWidth;
			var _availWindowHeight;

			function storeLastDivider(text) {
				if (!text)
					throw Error("Logic Error: 'storeLastDivider' should never be called with an empty parameter");
				var dividerChar = text.substring(0, 1);
				if (!_lastDividerString || !_lastDividerString.startsWith(dividerChar)) {
					window.KeyboardMaestro.SetVariable(_lastDividerCharVarName, dividerChar);
				}
				_lastDividerString = text;
			}

			function setupUI() {
				function getDividerString() {
					var divider = "";
					var longestMacroName = 0;
					_macrosInfo.macros.forEach(function(macro) {
						longestMacroName = Math.max(longestMacroName, macro.macroName.length);
						var matches = macro.macroNameNoPrefix.match(/([^0-9a-z ]{8,})/gi);
						if (matches) {
							matches.forEach(function(match) {
								if (match.length &gt; divider.length)
									divider = match;
							});
						}
					});

					if (!divider) {
						var dividerChar = getKMVariable(_lastDividerCharVarName) || "?";
						divider = Array(longestMacroName + 1).join(dividerChar);
					}
					return divider;
				}

				// setupUI()
				try {
					document.onkeydown = function(event) {
						if (event.which == 27 &amp;&amp; _editBoxDialogShowing)
							return false;
					};

					document.getElementById("header").innerText = "Organizing Macros for Palette\n\"" + _macrosInfo.groupName + "\"";
					storeLastDivider(getDividerString());
					// fillSelect();
					document.getElementById("selMacros").focus();
					enableControls();
				} catch (e) {
					ErrorHandler.handle(e, "setupUI");
				}
			} // setupUI()

			function KMDidShowWindow() {
				try {
					var actualScreenY = window.screenY;
					if (actualScreenY !== _desiredWindowPosition.top)
						window.moveBy(0, _desiredWindowPosition.top - actualScreenY);

					setupUI();
				} catch (e) {
					ErrorHandler.handle(e, "KMDidShowWindow");
				}
			}

			function KMWindow() {
				function setupAvailWindowWidthAndHeight() {
					function getFauxOrRealValue(varName, actualValue, actualOffsetValue) {
						var result;
						var str = getKMVariable(varName);
						if (str) {
							result = parseInt(str);
							if (isNaN(result))
								throw Error(varName + " contains non-numeric value '" + str + "'");
							result -= actualOffsetValue;
						} else {
							result = actualValue;
						}
						return result;
					}

					// setupAvailWindowWidthAndHeight()
					_availWindowWidth = getFauxOrRealValue("palorg_fauxAvailWindowWidth", window.screen.availWidth, 0);
					_availWindowHeight = getFauxOrRealValue("palorg_fauxAvailWindowHeight", window.screen.availHeight, window.screen.availTop);
				} // setupAvailWindowWidthAndHeight()

				function setupBestWindowHeight() {

					function calculateWindowHeightFromTblClientHeight(tblClient) {
						var headerAndFooterHeight = 140;
						return tblClient.offsetHeight + headerAndFooterHeight;
					}

					var minSelMacrosSize = 10;

					var tblClient = document.getElementById("tblClient");
					var selMacros = document.getElementById("selMacros");

					var size = Math.max(minSelMacrosSize, _macrosInfo.macros.length + 5);
					selMacros.size = size.toString();
					while ((size &gt; minSelMacrosSize) &amp;&amp; (calculateWindowHeightFromTblClientHeight(tblClient) &gt; _availWindowHeight)) {
						size = size - 1;
						selMacros.size = size.toString();
					}

					return calculateWindowHeightFromTblClientHeight(tblClient);
				}

				function setupWidth() {
					var sel = document.getElementById("selMacros");
					var selWidth = sel.offsetWidth;
					sel.style.width = "100%";
					var additionalSelWidth = Math.max(0, selWidth - sel.offsetWidth);
					return document.getElementById("content").offsetWidth + additionalSelWidth;
				}

				function calculateCenteredWindowPosition(width, height) {
					var availTop = window.screen.availTop;
					var availLeft = window.screen.availLeft;
					return {
						left: ((_availWindowWidth - width) / 2) + availLeft,
						top: ((_availWindowHeight - height) / 2) + availTop,
						width: width,
						height: height
					};
				}

				// KMWindow()
				try {
					setupAvailWindowWidthAndHeight();
					_macrosInfo = JSON.parse(getKMVariable("palorg_MacrosInfo", true));
					fillSelect();

					var width = setupWidth();
					document.body.style.width = width + "px";
					var height = setupBestWindowHeight();
					document.body.style.height = height + 'px';

					_desiredWindowPosition = calculateCenteredWindowPosition(width, height);
					return _desiredWindowPosition.left + ", " + _desiredWindowPosition.top + ", " + width + ", " + height;
				} catch (e) {
					ErrorHandler.handle(e, "KMWindow");
				}
			} // KMWindow()

			function submitWindow(result) {
				function storeResult() {
					try {
						var result = [];
						var options = document.getElementById("selMacros").options;
						var lastPrefixNumber = 0;
						for (var i = 0; i &lt; options.length; i++) {
							var option = options[i];
							var macro = _macrosInfo.macros[option.value];
							if (!macro.prefixNumber || macro.prefixNumber &lt;= lastPrefixNumber)
								return "Macros need to be re-numbered.";
							if (macro.macroName !== getMacroName(macro))
								result.push(macro.macroUUID + "\t" + macro.macroName + "\t" + macro.prefixNumber + ")" + macro.macroNameNoPrefix);
							lastPrefixNumber = macro.prefixNumber;
						}

						window.KeyboardMaestro.SetVariable("palorg_RenumberedMacrosList", result.join("\n"));

						window.KeyboardMaestro.SetVariable("palorg_GroupUUID", _macrosInfo.groupUUID);
						window.KeyboardMaestro.SetVariable("palorg_GroupName", _macrosInfo.groupName);
						window.KeyboardMaestro.SetVariable("palorg_GroupToggleMacroUID", _macrosInfo.groupToggleMacroUID);
						return "";
					} catch (e) {
						return e.message;
					}
				}

				// submitWindow()
				var errorMessage = storeResult();
				if (errorMessage) {
					showInfoDialog(errorMessage);
					return;
				}
				window.KeyboardMaestro.Submit(result);
			} // submitWindow()

			function cancelWindow(result) {
				window.KeyboardMaestro.Cancel(result);
			}
		&lt;/script&gt;
	&lt;/head&gt;

